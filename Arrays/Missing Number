public class missingNumber {
  public static void main(String[] args) {
    int arr[] = {1,2,4,5};
    int N = 5;
    System.out.println("The missing number is :" + missingNumber( arr,N));
  }
  public static int missingNumber(int arr[] , int N){
    sum = (N*(N+1))/2;
    s2 = 0;
    for(int i=0; i<N-1; i++){
    s2 += arr[i];
    
  }
  return sum-s2;
  }
}
